{
  "expo": {
    "name": "EmanciTech",
    "slug": "EmanciTech",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Brolink-Outline.ttf",
            "./assets/fonts/Brolink-Regular.ttf",
            "./assets/fonts/Informe-Bold.ttf",
            "./assets/fonts/Informe-Light.ttf",
            "./assets/fonts/Informe-Medium.ttf",
            "./assets/fonts/Informe-Regular.ttf",
            "./assets/fonts/Sogtric-Outline.otf",
            "./assets/fonts/Sogtric-Regular.otf",
            "./assets/fonts/Veltron-Regular.ttf"
          ]
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arjunkhetia.emancitech"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9792a25c-120a-4832-985b-bca8fafcda6c"
      }
    }
  }
}
